/*案例分析-数据  */
.case_text {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
}
.case_text {
  color: #6F6F6F;
}
.case_text text {
  color: #1C1C1C;
  font-weight: 600;
   font-size: 28rpx; 
}
.case_row {
  padding: 20rpx;
  border-bottom: 15rpx solid #EDEDED;
  background: #fff;
}
.case_time text{
  float: right;
  margin-top:10rpx;
  color: #919191;
}
.case_time {
  width: 100%;
  height: 40rpx;
}
.case_data {
  height: 100%;
  margin-bottom: 30rpx;
}
page {
  font-size: 30rpx;
  font-family: "微软雅黑";
  background: #EDEDED;
}
